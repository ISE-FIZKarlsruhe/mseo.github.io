<oops:AffectedElement>https://material-digital.de/pmdco/hasQuantitativeNominalSecondaryParameter</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasQuantitativeSecondaryMeasurement</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasProcessNodeComponent</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasQuantitativeMetadataMeasurement</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasInputObject</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasQuantitativeNominalPrimaryParameter</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/measuredProcessDuration</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasQuantitativeNominalMetadataParameter</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasQuantitativeNominalParameter</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/nominalProcessDuration</oops:AffectedElement>
<oops:AffectedElement>https://material-digital.de/pmdco/hasQuantitativePrimaryMetadata</oops:AffectedElement>
