                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty bfo:BFO_0000055 ;
                                                                                    owl:someValuesFrom [ owl:intersectionOf ( bfo:BFO_0000034
                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                owl:onProperty iof-core:functionOf ;
                                                                                                                                owl:someValuesFrom iof-core:MaterialArtifact
                                                                                                                              ]
                                                                                                                            ) ;
                                                                                                         rdf:type owl:Class
                                                                                                       ]
