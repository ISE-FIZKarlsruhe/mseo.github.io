###  https://spec.industrialontologies.org/ontology/core/Core/RawMaterialRole
iof-core:RawMaterialRole rdf:type owl:Class ;
                         rdfs:subClassOf bfo:BFO_0000023 ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty iof-core:roleOf ;
                                           owl:someValuesFrom [ owl:intersectionOf ( bfo:BFO_0000040
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty bfo:BFO_0000056 ;
                                                                                       owl:someValuesFrom [ owl:intersectionOf ( iof-core:BuyingBusinessProcess
